<h1>Add an Event</h1>
<p><%= simple_form_for @event do |f|  %>
    <%= f.input :title, class: "meetup_location" %><br>
    <%= f.input :description %><br>
    <%= f.input :eventstart, as: :date, start_year: Date.today.year %>
    <%= f.submit "Schedule", class: "btn btn-primary" %>
    <% end %></p>
